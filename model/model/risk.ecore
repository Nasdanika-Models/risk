  <eClassifiers xsi:type="ecore:EEnum" name="DataClassification">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Classification of data handled by a product or component, determining the sensitivity level and applicable controls."/>
    </eAnnotations>
    <eLiterals name="Public">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Publicly available data with no restrictions."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Internal" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Internal use only; not for public disclosure."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Confidential" value="2">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Sensitive data requiring restricted access (e.g., customer PII, financial data)."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Restricted" value="3">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Highly sensitive data with strict access controls (e.g., payment card data, regulated data)."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="TopSecret" value="4">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Most sensitive classification requiring maximum security controls."/>
      </eAnnotations>
    </eLiterals>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TreatmentStrategy">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Strategy for treating (responding to) an identified risk."/>
    </eAnnotations>
    <eLiterals name="Accept">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Accept the risk without additional controls; typically used for low-impact or low-likelihood risks."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Avoid" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Eliminate the risk by not engaging in the activity that creates it."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Mitigate" value="2">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reduce the likelihood or impact of the risk through controls."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Transfer" value="3">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Transfer the risk to a third party (e.g., insurance, outsourcing, contracts)."/>
      </eAnnotations>
    </eLiterals>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TreatmentStatus">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
