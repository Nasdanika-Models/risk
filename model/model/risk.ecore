  <eClassifiers xsi:type="ecore:EEnum" name="ControlType">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="The nature of a risk control."/>
    </eAnnotations>
    <eLiterals name="Preventive">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Prevents a risk event from occurring (e.g., access controls, encryption)."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Detective" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Detects when a risk event has occurred (e.g., monitoring, audits, alerts)."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Corrective" value="2">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Corrects the situation after a risk event (e.g., incident response, backups)."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="Compensating" value="3">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Provides an alternative control when a primary control cannot be implemented."/>
      </eAnnotations>
    </eLiterals>
